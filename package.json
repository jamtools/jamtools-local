{
  "name": "jamtools",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run build",
    "start": "node dist/src/index.js",
    "dev": "nodemon dist/src/index.js",
    "build": "tsc -p .",
    "build:watch": "tsc -p . --watch",
    "test": "jest",
    "check": "eslint --ext .js,.jsx,.tsx,.ts ./src --quiet --cache && eslint --ext .js,.jsx,.tsx,.ts ./webapp/src --quiet --cache",
    "fix": "eslint --ext .js,.jsx,.tsx,.ts ./src --quiet --fix --cache && eslint --ext .js,.jsx,.tsx,.ts ./webapp/src --quiet --fix --cache",
    "check-types": "tsc -b",
    "erase_flash": "esptool.py --port /dev/tty.usbserial-0001 erase_flash",
    "write_flash": "esptool.py --chip esp32 --port /dev/tty.usbserial-0001 write_flash -z 0x1000 esp32-20220618-v1.19.1.bin",
    "screen": "screen /dev/tty.usbserial-0001 115200",
    "screen_list": "screen -list",
    "kill_screen": "screen -X -S 10184 quit",
    "full_deploy": "ampy -p /dev/tty.usbserial-0001 put constants.py && ampy -p /dev/tty.usbserial-0001 put main.py",
    "deploy": "ampy -p /dev/tty.usbserial-0001 put main.py"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "debounce": "^1.2.1",
    "easymidi": "^3.0.1",
    "rxjs": "^7.8.0",
    "wled-client": "^0.22.1"
  },
  "devDependencies": {
    "@types/node": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.21.0",
    "eslint-plugin-import": "^2.26.0",
    "nodemon": "^2.0.19",
    "parcel-bundler": "^1.12.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
